<?xml version="1.0" encoding="UTF-8"?>
<project name="global_properties">
	<include file="local_properties.xml" optional="false"/>

	<property name="fiatlux.lib.dir" value="${fiatlux.root.dir}/lib" />	
	<property name="fiatlux.solution.dir" value="${fiatlux.root.dir}/trunk" />
	<property name="fiatlux.domain.dir" value="${fiatlux.solution.dir}/se.qxx.fiatlux.domain" />
	<property name="fiatlux.domain.src.dir" value="${fiatlux.domain.dir}/src" />	

	<property name="fiatlux.deploy.server" value="ULTRA" />
	<property name="fiatlux.deploy.server.path" value="/c/media/Dev/src/fiatlux" />
	<property name="fiatlux.deploy.server.path.apk" value="/c/media/Dev/src/jukebox-apk" />
	<property name="fiatlux.deploy.user" value="chris" />
	<property name="fiatlux.deploy.path" value="${fiatlux.deploy.user}@${fiatlux.deploy.server}:${fiatlux.deploy.server.path}" />

	<property name="fiatlux.apk.deploy.path" value="${fiatlux.deploy.user}@${fiatlux.deploy.server}:${fiatlux.deploy.server.path.apk}" />

	<path id="fiatlux.classpath">
        	<fileset dir="${fiatlux.lib.dir}" includes="**/*.jar"/>
    	</path>
	
</project>
