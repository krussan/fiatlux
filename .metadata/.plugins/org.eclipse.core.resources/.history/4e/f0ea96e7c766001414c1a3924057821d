package se.qxx.fiatlux.server;

public class FiatLuxServer {
	public static void main(String args[]) {
		checkArguments(args);
		
		setupListening();
		
	}
	
	public static void checkArguments(String args[]) {
		if (args.length > 0) {
			printHelp();
			exit();
		}
	}
	
	public static void printHelp() {
		System.out.println("HELP!");
	}
}
